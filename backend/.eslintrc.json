{
  "extends": [
    "eslint:recommended",
    "plugin:node/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
    }
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off"
  },
  "env": {
    "node": true,
    "es2022": true
  }
}
